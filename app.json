{
    "expo": {
        "name": "Freebee",
        "slug": "Freebee",
        "version": "2.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "freebee.kz",
            "infoPlist": {
                "NSCameraUsageDescription": "This app uses the camera to scan barcodes on draws tickets."
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            },
            "package": "freebee.kz",
            "versionCode": 2
        },
        "web": {
            "favicon": "./assets/images/favicon.png"
        }
    }
}
